{
    "rukus": [
        {
            "chapter": 26,
            "verse": 105,
            "text": "كذبتۡ قوۡم نوح ٱلۡمرۡسلين"
        },
        {
            "chapter": 26,
            "verse": 106,
            "text": "إذۡ قال لهمۡ أخوهمۡ نوح ألا تتقون"
        },
        {
            "chapter": 26,
            "verse": 107,
            "text": "إني لكمۡ رسول أمينٞ"
        },
        {
            "chapter": 26,
            "verse": 108,
            "text": "فٱتقوا ٱلله وأطيعون"
        },
        {
            "chapter": 26,
            "verse": 109,
            "text": "ومآ أسۡـٔلكمۡ عليۡه منۡ أجۡرۖ إنۡ أجۡري إلا علىٰ رب ٱلۡعٰلمين"
        },
        {
            "chapter": 26,
            "verse": 110,
            "text": "فٱتقوا ٱلله وأطيعون"
        },
        {
            "chapter": 26,
            "verse": 111,
            "text": "۞قالوٓا أنؤۡمن لك وٱتبعك ٱلۡأرۡذلون (الأرذلون: السفلة من الناس)"
        },
        {
            "chapter": 26,
            "verse": 112,
            "text": "قال وما علۡمي بما كانوا يعۡملون"
        },
        {
            "chapter": 26,
            "verse": 113,
            "text": "إنۡ حسابهمۡ إلا علىٰ ربيۖ لوۡ تشۡعرون"
        },
        {
            "chapter": 26,
            "verse": 114,
            "text": "ومآ أنا بطارد ٱلۡمؤۡمنين"
        },
        {
            "chapter": 26,
            "verse": 115,
            "text": "إنۡ أنا إلا نذيرٞ مبينٞ"
        },
        {
            "chapter": 26,
            "verse": 116,
            "text": "قالوا لئن لمۡ تنته يٰنوح لتكونن من ٱلۡمرۡجومين (المرجومين: المقتولين رميا بالحجارة)"
        },
        {
            "chapter": 26,
            "verse": 117,
            "text": "قال رب إن قوۡمي كذبون"
        },
        {
            "chapter": 26,
            "verse": 118,
            "text": "فٱفۡتحۡ بيۡني وبيۡنهمۡ فتۡحا ونجني ومن معي من ٱلۡمؤۡمنين (فافتح: احكم)"
        },
        {
            "chapter": 26,
            "verse": 119,
            "text": "فأنجيۡنٰه ومن معه في ٱلۡفلۡك ٱلۡمشۡحون (المشحون: المملوء بالناس، والدواب، والمتاع)"
        },
        {
            "chapter": 26,
            "verse": 120,
            "text": "ثم أغۡرقۡنا بعۡد ٱلۡباقين"
        },
        {
            "chapter": 26,
            "verse": 121,
            "text": "إن في ذٰلك لأٓيةۖ وما كان أكۡثرهم مؤۡمنين"
        },
        {
            "chapter": 26,
            "verse": 122,
            "text": "وإن ربك لهو ٱلۡعزيز ٱلرحيم"
        }
    ]
}